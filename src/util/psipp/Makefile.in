
srcdir = @srcdir@
top_srcdir = @top_srcdir@
top_objdir = ../../..
MKDIRS = @MKDIRS@
VPATH = @srcdir@

prefix = @prefix@
incdir = @incdir@
target = @target@
objincdir = @objincdir@
exec_prefix = @exec_prefix@
bindir = @bindir@

YACC = @YACC@
INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@

COPT = @COPT@
CINC = @CINC@
CDBG = @CDBG@
COTH = @COTH@
CDEF = @CDEF@
CFLAGS = @CFLAGS@

CPPFLAGS = -I.

CC = @CC@

DODEPEND = @DODEPEND@

OBJ = \
error.o \
if.o \
include.o \
main.o \
name.o \
operator.o \
parse.o \
scan.o \
print.o

IOBJ = \
ipsipp.o

.PHONY:	all default install intall_target install_inc clean oclean dclean targetclean

all: default
default: psipp ipsipp

psipp: $(OBJ)
	$(CC) $(CFLAGS) -o $@  $(OBJ)

ipsipp: $(IOBJ)
	$(CC) $(CFLAGS) -o ipsipp $(IOBJ)

parse.c: parse.y
	$(YACC) -v -d $<
	mv y.tab.c parse.c
	mv y.tab.h parse.h

parse.h: parse.c

install: default
	$(MKDIRS) $(bindir) 
	$(INSTALL_PROGRAM) psipp $(bindir)
	$(INSTALL_PROGRAM) ipsipp $(bindir)

install_inc:

clean:
	rm -f *.o

targetclean:
	rm -f psipp ipsipp

ifneq ($(DODEPEND),no)
$(OBJ:.o=.d): parse.h
include $(OBJ:.o=.d)
else
$(OBJ): parse.h
endif

$(top_srcdir)/configure: $(top_srcdir)/configure.in $(top_srcdir)/aclocal.m4
	cd $(top_srcdir) && autoconf

$(top_objdir)/config.status: $(top_srcdir)/configure
	cd $(top_objdir) && ./config.status --recheck

Makefile: $(srcdir)/Makefile.in $(top_objdir)/config.status
	cd $(top_objdir) && CONFIG_FILES=src/util/psipp/Makefile ./config.status
